{
  "name": "COMPONENTID",
  "version": "1.0.0",
  "description": "An advanced OpenDevStack Frontend Quickstarter to build desktop, mobile and pwa apps with the ionic framework and react.",
  "author": {
    "name": "Simon Golms",
    "email": "development@gol.ms",
    "url": "https://gol.ms"
  },
  "private": true,
  "engines": {
    "npm": ">=8.5.5",
    "node": ">=16.15"
  },
  "scripts": {
    "build": "vite build",
    "generate:assets": "capacitor-assets generate",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx || stylelint '**/*.css'",
    "lint:fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx || stylelint --fix '**/*.css'",
    "prepare": "husky install && chmod ug+x .husky/* && playwright install",
    "release": "semantic-release --extends './scripts/releaserc.release.js'",
    "release:version": "semantic-release --extends './scripts/releaserc.version.js' --dry-run --no-ci",
    "repair": "rm -rf .git/hooks build node_modules playwright/.cache package-lock.json && npm install",
    "start": "vite",
    "test": "playwright test --config 'playwright-ct.config.ts'"
  },
  "dependencies": {
    "@azure/msal-browser": "2.32.1",
    "@azure/msal-react": "1.5.1",
    "@capacitor/app": "4.1.1",
    "@capacitor/core": "4.6.1",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/keyboard": "4.1.0",
    "@capacitor/preferences": "4.0.2",
    "@capacitor/status-bar": "4.1.1",
    "@ionic/react": "6.4.2",
    "@ionic/react-router": "6.4.2",
    "@reduxjs/toolkit": "1.9.1",
    "ionicons": "6.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "5.3.4",
    "redux-persist": "6.0.0",
    "web-vitals": "3.1.1"
  },
  "devDependencies": {
    "@boehringer-ingelheim/prettier-config": "1.0.0",
    "@capacitor/assets": "2.0.4",
    "@capacitor/cli": "4.6.1",
    "@commitlint/cli": "17.4.1",
    "@commitlint/config-conventional": "17.4.0",
    "@ionic/cli": "6.20.6",
    "@microsoft/microsoft-graph-types": "2.25.0",
    "@playwright/experimental-ct-react": "1.29.2",
    "@playwright/test": "1.29.2",
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "@vitejs/plugin-react": "3.0.1",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.2",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "0.11.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sonarjs": "0.18.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.2",
    "semantic-release": "19.0.5",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "29.0.0",
    "stylelint-order": "6.0.1",
    "stylelint-prettier": "2.0.0",
    "typescript": "4.9.4",
    "vite": "4.0.4",
    "vite-plugin-pwa": "0.14.1"
  },
  "overrides": {
    "@azure/msal-common": "9.0.1"
  }
}
