{
  "name": "COMPONENTID",
  "version": "1.0.0",
  "description": "An advanced OpenDevStack Frontend Quickstarter to build desktop, mobile and pwa apps with the ionic framework and react.",
  "author": {
    "name": "Simon Golms",
    "email": "development@gol.ms",
    "url": "https://gol.ms"
  },
  "private": true,
  "engines": {
    "npm": ">=9.3",
    "node": ">=18.14"
  },
  "scripts": {
    "build": "vite build",
    "generate:assets": "capacitor-assets generate",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx || stylelint 'src/**/*.css'",
    "lint:fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx || stylelint --fix 'src/**/*.css'",
    "prepare": "husky install && chmod ug+x .husky/* && playwright install",
    "release": "semantic-release --extends './scripts/releaserc.release.js'",
    "release:version": "semantic-release --extends './scripts/releaserc.version.js' --dry-run --no-ci",
    "repair": "rm -rf .git/hooks build node_modules playwright/.cache package-lock.json && npm install",
    "start": "vite",
    "test": "playwright test --config 'playwright-ct.config.ts'"
  },
  "dependencies": {
    "@azure/msal-browser": "2.36.0",
    "@azure/msal-react": "1.5.6",
    "@capacitor/app": "4.1.1",
    "@capacitor/core": "4.7.3",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/keyboard": "4.1.1",
    "@capacitor/preferences": "4.0.2",
    "@capacitor/status-bar": "4.1.1",
    "@ionic/react": "7.0.3",
    "@ionic/react-router": "7.0.3",
    "@reduxjs/toolkit": "1.9.5",
    "ionicons": "7.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "5.3.4",
    "redux-persist": "6.0.0",
    "web-vitals": "3.3.1"
  },
  "devDependencies": {
    "@boehringer-ingelheim/eslint-config": "1.1.3",
    "@boehringer-ingelheim/prettier-config": "1.0.0",
    "@capacitor/assets": "2.0.4",
    "@capacitor/cli": "5.0.0-beta.2",
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@ionic/cli": "7.0.1",
    "@microsoft/microsoft-graph-types": "2.26.0",
    "@playwright/experimental-ct-react": "1.32.3",
    "@playwright/test": "1.32.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/node": "18.15.13",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@types/react-router": "5.1.20",
    "@types/react-router-dom": "5.3.3",
    "@vitejs/plugin-react": "4.0.0",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "semantic-release": "21.0.1",
    "stylelint": "15.5.0",
    "stylelint-config-standard": "33.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-prettier": "3.0.0",
    "typescript": "5.0.4",
    "vite": "4.3.1",
    "vite-plugin-pwa": "0.14.7"
  },
  "overrides": {
    "xml2js": "^0.5.0"
  }
}
