{
  "name": "COMPONENTID",
  "version": "1.0.0",
  "description": "An advanced OpenDevStack Frontend Quickstarter to build desktop, mobile and pwa apps with the ionic framework and react.",
  "author": {
    "name": "Simon Golms",
    "email": "development@gol.ms",
    "url": "https://gol.ms"
  },
  "private": true,
  "engines": {
    "npm": ">=8.5.5",
    "node": ">=16.15"
  },
  "scripts": {
    "build": "vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx || stylelint '**/*.css'",
    "lint:fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx || stylelint --fix '**/*.css'",
    "prepare": "husky install && chmod ug+x .husky/* && playwright install",
    "release": "semantic-release --extends './scripts/releaserc.release.js'",
    "release:version": "semantic-release --extends './scripts/releaserc.version.js' --dry-run --no-ci",
    "repair": "rm -rf .git/hooks build node_modules playwright/.cache package-lock.json && npm install",
    "start": "vite",
    "test": "playwright test --config 'playwright-ct.config.ts'"
  },
  "dependencies": {
    "@azure/msal-browser": "2.28.1",
    "@azure/msal-react": "1.4.5",
    "@capacitor/app": "4.0.1",
    "@capacitor/core": "4.1.0",
    "@capacitor/haptics": "4.0.1",
    "@capacitor/keyboard": "4.0.1",
    "@capacitor/preferences": "4.0.1",
    "@capacitor/status-bar": "4.0.1",
    "@ionic/react": "6.2.4",
    "@ionic/react-router": "6.2.4",
    "@reduxjs/toolkit": "1.8.5",
    "ionicons": "6.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "react-router": "5.3.3",
    "react-router-dom": "5.3.3",
    "redux-persist": "6.0.0",
    "web-vitals": "3.0.0",
    "workbox-background-sync": "6.5.4",
    "workbox-broadcast-update": "6.5.4",
    "workbox-cacheable-response": "6.5.4",
    "workbox-core": "6.5.4",
    "workbox-expiration": "6.5.4",
    "workbox-google-analytics": "6.5.4",
    "workbox-navigation-preload": "6.5.4",
    "workbox-precaching": "6.5.4",
    "workbox-range-requests": "6.5.4",
    "workbox-routing": "6.5.4",
    "workbox-strategies": "6.5.4",
    "workbox-streams": "6.5.4"
  },
  "devDependencies": {
    "@capacitor/cli": "4.1.0",
    "@commitlint/cli": "17.1.1",
    "@commitlint/config-conventional": "17.1.0",
    "@ionic/cli": "6.20.1",
    "@microsoft/microsoft-graph-types": "2.23.0",
    "@playwright/experimental-ct-react": "1.25.1",
    "@playwright/test": "1.25.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/node": "18.7.13",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-router": "5.1.18",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/parser": "5.35.1",
    "@vitejs/plugin-react": "2.0.1",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-import-resolver-typescript": "3.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-no-type-assertion": "1.3.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sonarjs": "0.15.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "stylelint": "14.11.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "28.0.0",
    "stylelint-order": "5.0.0",
    "stylelint-prettier": "2.0.0",
    "typescript": "4.8.2",
    "vite": "3.0.9"
  }
}
